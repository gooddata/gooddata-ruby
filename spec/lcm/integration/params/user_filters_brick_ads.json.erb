{
  "organization": "<%= config[:prod_organization] %>",
  "GDC_PROJECT_ID": "<%= project_id %>",
  "CLIENT_GDC_PROTOCOL": "https",
  "CLIENT_GDC_HOSTNAME": "<%= config[:prod_server] %>",
  "gd_encoded_params": {
    "GDC_USERNAME": "${username}",
    "GDC_PASSWORD": "${password}",
    "filters_config": {
      "user_column": "login",
      "labels": [{"label": "label.csv_policies.state", "column": "state"}]
    },
    "ads_client": {
      "username": "${username}",
      "password": "${password}",
      "jdbc_url": "<%= jdbc_url %>"
    },
    "input_source": {
      "type": "ads",
      "query": "select login, state, client_id from user_filters;"
    },
    "users_brick_config": {
      "input_source": {
        "type": "ads",
        "query": "select login from user_filters;"
      }
    }
  },
  "fail_early": "true",
  "strict": "true",
  "username": "<%= config[:username] %>",
  "password": "<%= config[:password] %>",
  "sync_mode": "<%= sync_mode %>",
  "data_product": "<%= data_product %>"
}
