{
  "organization": "<%= config[:prod_organization] %>",
  "GDC_PROJECT_ID": "<%= project_id %>",
  "CLIENT_GDC_PROTOCOL": "https",
  "CLIENT_GDC_HOSTNAME": "<%= config[:prod_server] %>",
  "gd_encoded_params": {
    "GDC_USERNAME": "${username}",
    "GDC_PASSWORD": "${password}",
    "filters_config": {
      "user_column": "login",
      "labels": <%= label_config %>
    },
    <% if data_source == 's3' %>
    "aws_client": {
      "access_key_id": "foo",
      "secret_access_key": "foo",
      "endpoint": "<%= s3_endpoint %>"
    },
    "input_source": {
      "type": "s3",
      "key": "<%= s3_key %>",
      "bucket": "<%= s3_bucket %>"
    },
    <% elsif data_source =='ads' %>
    "ads_client": {
      "username": "<%= ads_username %>",
      "password": "<%= ads_password %>",
      "jdbc_url": "<%= jdbc_url %>"
    },
    "input_source": {
      "type": "ads",
      "query": "<%= ads_query %>"
    },
    <% end %>
    "users_brick_config": {
      <% if data_source == 's3' %>
      "input_source": {
        "type": "s3",
        "key": "<%= users_brick_input[:s3_key] %>",
        "bucket": "<%= users_brick_input[:s3_bucket] %>"
      },
      "login_column": "custom_login"
      <% elsif data_source =='ads' %>
      "ads_client": {
        "username": "<%= ads_username %>",
        "password": "<%= ads_password %>",
        "jdbc_url": "<%= jdbc_url %>"
      },
      "input_source": {
        "type": "ads",
        "query": "<%= usrs_ads_query %>"
      }
      <% end %>
    }
  },
  "fail_early": "true",
  "strict": "true",
  "username": "<%= config[:username] %>",
  "password": "<%= config[:password] %>",
  "sync_mode": "<%= sync_mode %>",
  "data_product": "<%= data_product %>"
}
